#include <iostream>
#include <stdlib.h>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int k, n, kmax, kmin;
    cin>>k;
    cin>>n;
    int arr[n], i;
    for(i=0; i<n; i++)
    {
        cin>>arr[i];
    }
    sort(arr, arr+n);
    cout<<"smallest : "<<arr[k-1]<<endl;
    cout<<"largest : "<<arr[n-k]<<endl;
       return 0;
}
