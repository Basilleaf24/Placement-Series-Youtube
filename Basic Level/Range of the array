#include <iostream>
#include <stdlib.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n], i, min, max;
    for(i=0; i<n; i++)
    {
        cin>>arr[i];
    }
    min=max=arr[0];
    for(i=0; i<n; i++)
    {
        if(arr[i]>max)
            max=arr[i];
        if(arr[i]<min)
            min=arr[i];
    }
        cout<<max-min<<endl;

       return 0;
}
