#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int c[n], i, miss, sum=0;
	    
	    for(i=0; i<n-1; i++)
	        cin>>c[i];
	     for(i=0; i<n-1; i++)
	        sum+=c[i]; 
	    
	    cout<<(n*(n+1)/2)-sum<<endl;
	}
	return 0;
}

